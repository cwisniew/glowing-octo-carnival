{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "extends": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"]

    },
    "ignorePatterns": [
        "**/*.d.ts"
    ]
}
